#. Name of the plugin
msgid "Content expiry"
msgstr "Caducidad del contenido"

#: src/Plugin.php:68
msgid "Current server time"
msgstr "Hora actual del servidor"

#. Author of the plugin
msgid "Daniel Lúcia"
msgstr "Daniel Lúcia"

#: src/Plugin.php:58
msgid "Date and time of expiry:"
msgstr "Fecha y hora de expiración:"

#: src/Plugin.php:154
msgid "days"
msgstr "días"

#: src/Plugin.php:53
msgid "Enable content expiry for this post"
msgstr "Activar la caducidad del contenido de este post"

#. Description of the plugin
msgid "Expira el contenido después de un tiempo determinado."
msgstr "Expira el contenido después de un tiempo determinado."

#: src/Plugin.php:29
msgid "Expiry date"
msgstr "Fecha de caducidad"

#: src/Plugin.php:155
msgid "hours"
msgstr "horas"

#. Author URI of the plugin
msgid "http://www.daniellucia.es"
msgstr "http://www.daniellucia.es"

#: src/Plugin.php:156
msgid "minutes"
msgstr "minutos"

#: src/Plugin.php:157
msgid "seconds"
msgstr "segundos"

#: src/Plugin.php:63
msgid ""
"Set the date and time when this content will expire. After this date, the "
"content will be hidden and a message will be displayed."
msgstr ""
"Establezca la fecha y la hora en que expirará este contenido. Después de "
"esta fecha, el contenido se ocultará y se mostrará un mensaje."

#: src/Plugin.php:128 src/Plugin.php:131 src/Plugin.php:152
msgid "This content has expired."
msgstr "Este contenido ha caducado."

#: src/Plugin.php:131 src/Plugin.php:153
msgid "Time left:"
msgstr "Tiempo restante:"
